<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.backerror.rit.coursemanagment">

    <application
        android:allowBackup="true"
        android:icon="@drawable/course"
        android:label="@string/app_name"
        android:roundIcon="@drawable/course"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.LoginActivity" />
        <activity android:name=".Activity.RegistrationActivity"
            android:parentActivityName=".Activity.LoginActivity"/>
        <activity android:name=".Activity.StudentHomeActivity" />
        <activity android:name=".Activity.TeacherHomeActivity"></activity>
    </application>

</manifest>